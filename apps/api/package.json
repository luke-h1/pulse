{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:snapshot": "jest --updateSnapshot",
    "dev": "nodemon --exec \"node -r esbuild-register ./src/main.ts\" -e .ts",
    "start": "node ./dist/main.js",
    "gen-env": "gen-env-types .env -o src/types/env.d.ts -e .",
    "build": "tsc",
    "tsc": "tsc --noEmit",
    "lint": "eslint \"./src/**/*.{js, jsx,ts,tsx}\" --quiet --fix && eslint \"./**/*.{js, jsx,ts,tsx}\" --quiet --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,html}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,ts,tsx,html}\"",
    "generate": "pnpm prisma generate",
    "migrate:dev": "pnpm prisma migrate dev",
    "prisma:format": "pnpm prisma format",
    "studio": "pnpm prisma studio",
    "postinstall": "pnpm prisma generate"
  },
  "dependencies": {
    "@apollo/server-plugin-landing-page-graphql-playground": "^4.0.0",
    "@pothos/core": "^3.29.0",
    "@pothos/plugin-directives": "^3.10.2",
    "@pothos/plugin-mocks": "^3.7.8",
    "@pothos/plugin-prisma": "^3.47.2",
    "@pothos/plugin-tracing": "^0.5.8",
    "@prisma/client": "4.12.0",
    "apollo-server-express": "^3.6.7",
    "axios": "^1.4.0",
    "body-parser": "^1.19.2",
    "cloudinary": "^1.28.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.1",
    "got": "11.8.3",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.18.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pino": "8.4.1",
    "pino-pretty": "^9.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.12",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.2",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.10.0",
    "gen-env-types": "^1.3.4",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prisma": "4.16.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
