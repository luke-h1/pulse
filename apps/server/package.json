{
  "name": "server",
  "version": "1.0.0",
  "scripts": {
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:snapshot": "jest --updateSnapshot",
    "watch": "tsc -w",
    "dev": "cross-env NODE_ENV=development nodemon dist/main.js",
    "start": "node ./dist/main.js",
    "gen-env": "gen-env-types .env -o src/types/env.d.ts -e .",
    "build": "tsc",
    "tsc": "tsc --noEmit",
    "lint": "eslint \"./src/**/*.{js, jsx,ts,tsx}\" --quiet --fix --ignore-pattern \"!./src/prisma\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,html}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,ts,tsx,html}\"",
    "db:generate": "pnpm prisma generate",
    "db:migrate": "pnpm prisma migrate dev",
    "db:format": "pnpm prisma format",
    "db:studio": "pnpm prisma studio",
    "postinstall": "pnpm prisma generate"
  },
  "dependencies": {
    "@apollo/server": "^4.9.3",
    "@escape.tech/graphql-armor": "^2.2.0",
    "@prisma/client": "5.2.0",
    "@prisma/generator-helper": "^5.1.1",
    "@types/busboy": "^1.5.0",
    "apollo-server-cache-redis": "^3.3.1",
    "apollo-server-core": "3.12.1",
    "apollo-server-express": "^3.12.0",
    "aws-sdk": "^2.991.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "busboy": "^1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudinary": "1.29.0",
    "compression": "^1.7.4",
    "connect-redis": "^6.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dataloader": "^2.2.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "form-data": "^4.0.0",
    "graphql": "^16.6.0",
    "graphql-fields": "^2.0.3",
    "graphql-hooks": "workspace:*",
    "graphql-scalars": "^1.22.2",
    "graphql-tools": "^9.0.0",
    "graphql-upload": "^12.0.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "pino": "8.15.0",
    "pino-pretty": "^10.2.0",
    "reading-time": "^1.5.0",
    "reflect-metadata": "^0.1.13",
    "slugify": "^1.6.6",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.1",
    "type-graphql": "2.0.0-beta.3",
    "typegraphql-prisma": "0.27.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@prisma/migrate": "^5.1.1",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/connect-redis": "^0.0.20",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/graphql-fields": "^1.3.5",
    "@types/graphql-upload": "^8.0.7",
    "@types/ioredis-mock": "^8.2.2",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.5.0",
    "esbuild-register": "^3.4.2",
    "gen-env-types": "^1.3.4",
    "ioredis-mock": "^8.8.3",
    "jest": "^29.6.2",
    "jest-environment-node": "^29.6.2",
    "jest-junit": "^16.0.0",
    "jest-matcher-utils": "^29.6.2",
    "jest-mock-extended": "^3.0.5",
    "jest-resolve": "^29.6.2",
    "jest-serializer-html": "^7.1.0",
    "nanoid": "4.0.2",
    "node-mocks-http": "^1.13.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "prisma": "^5.1.1",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}